{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "axios": "^0.17.1",
    "gatsby": "^1.9.110",
    "gatsby-link": "^1.6.27",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "gatsby-plugin-styled-components": "^2.0.2",
    "gatsby-plugin-typography": "^1.7.10",
    "gatsby-remark-prismjs": "^1.2.9",
    "gatsby-source-filesystem": "^1.5.8",
    "gatsby-transformer-remark": "^1.7.21",
    "react-transition-group": "^2.2.1",
    "styled-components": "^2.2.3",
    "typography-theme-bootstrap": "^0.16.7",
    "typography-theme-grand-view": "^0.15.10",
    "typography-theme-lawton": "^0.15.10",
    "typography-theme-st-annes": "^0.15.10",
    "typography-theme-sutro": "^0.15.10"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "./node_modules/.bin/tailwind build ./src/index.tailwind.css -o ./src/layouts/index.css && gatsby build",
    "develop": "./node_modules/.bin/tailwind build ./src/index.tailwind.css -o ./src/layouts/index.css && gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "deploy": "gatsby build && gh-pages-with-token -d public"
  },
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "prettier": "^1.8.2",
    "tailwindcss": "^0.2.2"
  }
}
